<Page x:Class="WinUICommunity.DemoApp.Pages.ContentDialogPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:wuc="using:WinUICommunity">

    <wuc:SettingsPageControl IsTabStop="False"
                             ModuleDescription="With the help of ShowAsyncQueue extension you can open multiple ContentDialogs."
                             ModuleImageSource="ms-appx:///Assets/Modules/PT.png"
                             ModuleTitle="ContentDialog: ShowAsyncQueue Extension">
        <wuc:SettingsPageControl.ModuleContent>
            <StackPanel ChildrenTransitions="{StaticResource SettingsCardsAnimations}">
                <Button HorizontalAlignment="Center"
                        Click="Button_Click"
                        Content="Open 5 ContentDialog" />
            </StackPanel>
        </wuc:SettingsPageControl.ModuleContent>
    </wuc:SettingsPageControl>
</Page>
