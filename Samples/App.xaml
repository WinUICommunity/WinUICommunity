<?xml version="1.0" encoding="utf-8" ?>
<Application x:Class="DemoApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:wui="using:WindowUI">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />
                <ResourceDictionary Source="ms-appx:///WindowUI/Themes/Generic.xaml" />
                <ResourceDictionary Source="ms-appx:///WindowUI.LandingPages/Themes/Generic.xaml" />
                <wui:ItemTemplates />
                <ResourceDictionary Source="Themes/ThemeResources.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
