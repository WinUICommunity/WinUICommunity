<Page x:Class="WindowUI.DemoApp.Pages.ContentDialogPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:wui="using:WindowUI">

    <wui:SettingsPageControl IsTabStop="False"
                             ModuleDescription="With the help of ShowAsyncQueue extension you can open multiple ContentDialogs."
                             ModuleImageSource="ms-appx:///Assets/Modules/PT.png"
                             ModuleTitle="ContentDialog: ShowAsyncQueue Extension">
        <wui:SettingsPageControl.ModuleContent>
            <StackPanel ChildrenTransitions="{StaticResource SettingsCardsAnimations}">
                <Button HorizontalAlignment="Center"
                        Click="Button_Click"
                        Content="Open 5 ContentDialog" />
            </StackPanel>
        </wui:SettingsPageControl.ModuleContent>
    </wui:SettingsPageControl>
</Page>
